<h5 class="pull-left">Serial Dilution</h5>
<div class="pull-right">
    <i class="fa fa-question-circle editorTabHelpIcon"></i>
</div>

<div class="form">
    <div class="form-group">
        <label class="control-label">Select Serial Dilution</label>
        <div class="">
            <select class="form-control">
                <option>Select from Template</option>
            </select>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label">Number of Doses</label>
        <div class="">
            <input type="text" class="form-control pull-left editorTabNumOfDoses" />
            <div class="btn-group" role="group">
                <button class="btn btn-default"><i class="fa fa-arrow-up"></i></button>
                <button class="btn btn-default"><i class="fa fa-arrow-right"></i></button>
                <button class="btn btn-default"><i class="fa fa-arrow-down"></i></button>
                <button class="btn btn-default"><i class="fa fa-arrow-left"></i></button>
            </div>
        </div>
    </div>
    <div>
        <div class="radio pull-left editorTabVolRadio">
            <label>
                <input type="radio" value="" />
            </label>
        </div>
        <div class="form-group pull-left editorTabTransferVol">
            <label class="control-label">Transfer Volume (uL)</label>
            <div class="">
                <input type="text" class="form-control" />
            </div>
        </div>
        <div class="form-group pull-left editorTabDestinationWellVol">
            <label class="control-label">Destination Well Volume (uL)</label>
            <div class="">
                <input type="text" class="form-control" />
            </div>
        </div>
    </div>
    <div class="form group">
        <div class="editorTabOr"><span>Or</span></div>
    </div>
    <div>
        <div class="radio pull-left editorTabDilutionFactorRadio">
            <label>
                <input type="radio" value="" />
            </label>
        </div>
        <div class="form-group pull-left editorTabDilutionFactor">
            <label class="control-label">Dilution Factor</label>
            <div class="">
                <input type="text" class="form-control" disabled />
            </div>
        </div>
    </div>
    <div class="form-group">
        <button class="btn btn-default pull-right">Apply Dilution</button>
    </div>
</div>