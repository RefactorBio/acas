<div class="modal fade" name="serviceCallModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <!--
            <div class="modal-header">

            </div>
            -->
            <div class="modal-body">
                <div class="" name="serviceCallProgressFeedback" style="text-align: center;">
                    <h5 class="modal-title bv_serviceCallProgressText" style="padding-bottom: 0.5em; padding-top: 1em;" ></h5>
                    <div class="progress">
                        <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                            <span class="sr-only"></span>
                        </div>
                    </div>
                </div>

                <div class="hide" name="serverErrorMessage">
                    <p>An error occurred processing the list of identifiers.  Please try again.</p>
                    <p>If you continue to receieve this error,
                        please contact <%= AppLaunchParams.adminEmail %>
                    </p>
                </div>

                <div class="hide" name="serviceControllerContainer"></div>

            </div>
            <div class="modal-footer">
                <div class="hide" name="warningButtons">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Go Back</button>
                    <button type="button" class="btn btn-primary" name="warningContinue">Continue</button>
                </div>
                <div class="hide" name="errorButtons">
                    <button type="button" class="btn btn-default" name="errorGoBack">Go Back</button>
                </div>
                <div class="hide" name="serverErrorButtons">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Go Back</button>
                </div>
                <div class="hide" name="closeButtons">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Okay</button>
                </div>

                <!--

                -->
            </div>
        </div>
    </div>
</div>